# AWS WorkMail EWS Test Configuration
# Copy this file to .env and fill in your credentials

# Your AWS WorkMail email address
EWS_USERNAME=your-email@example.com

# Your AWS WorkMail password
EWS_PASSWORD=your-password

# Your AWS WorkMail EWS endpoint URL
# Format: https://ews.mail.{region}.awsapps.com/EWS/Exchange.asmx
EWS_URL=https://ews.mail.eu-west-1.awsapps.com/EWS/Exchange.asmx

# Optional: Test email recipient (defaults to EWS_USERNAME)
EWS_TEST_EMAIL=
