{
  "name": "n8n-nodes-aws-workmail-ews",
  "version": "1.0.13",
  "description": "n8n node for AWS WorkMail using Exchange Web Services (EWS) protocol",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "aws",
    "workmail",
    "ews",
    "exchange"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zcode-apps/n8n-nodes-aws-workmail-ews",
  "author": {
    "name": "z:code",
    "email": "info@z-code.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcode-apps/n8n-nodes-aws-workmail-ews.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AwsWorkMailEwsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AwsWorkMailEws/AwsWorkMailEws.node.js",
      "dist/nodes/AwsWorkMailEws/AwsWorkMailEwsTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "ews-javascript-api": "^0.15.3",
    "lodash": "^4.17.21",
    "zod": "^4.3.5"
  }
}
